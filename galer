  export DEBIAN_FRONTEND=noninteractive
  ln -fs /usr/share/zoneinfo/Asia/Jakarta /etc/localtime
  export TZ=Asia/Jakarta
  export LC_CTYPE=en_US.UTF-8
  export LC_ALL=en_US.UTF-8
  apt update -y && apt upgrade -y && apt-get install -y --no-install-recommends tzdata && apt install sudo -y && apt install wget -y && apt install locales -y && apt-get install tmate -y
  sudo echo "LC_ALL=en_US.UTF-8" >> /etc/environment
  sudo echo "en_US.UTF-8 UTF-8" >> /etc/locale.gen
  sudo echo "LANG=en_US.UTF-8" > /etc/locale.conf
  locale-gen en_US.UTF-8
  source /etc/environment
  wget https://raw.githubusercontent.com/WisnuArdhi28/script/main/setup.sh && chmod +x setup.sh && sudo bash setup.sh
